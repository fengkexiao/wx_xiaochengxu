@import "./index.skeleton.wxss";

.title-box {
  background: rgba(255, 255, 255, 1);
  position: fixed;
  width: 100%;
  z-index: 11;
}

.title-default-box {
  display: flex;
  align-items: center;
  justify-content: center;
  background: url('https://com-shuibei-peach-static.oss-cn-shenzhen.aliyuncs.com/tmcpro/images/home/hone-header__doctor.png');
  background-size: 100% 100%;
  background-color: #5091ED;
}


.title-name-text {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 36rpx;
  flex-shrink: 0;
}

.unredcount {
  width: 28rpx;
  height: 22rpx;
  line-height: 22rpx;
  opacity: 0.85;
  background: #fe5853;
  border-radius: 12rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #ffffff;
  position: absolute;
  top: 0;
  text-align: center;
  left: 60rpx;
}

.title-defalut-text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.65);
  line-height: 24rpx;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-left: 16rpx;
}


.home-icon-box {
  position: absolute;
  left: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 100%;
  border: 0.25rpx solid #e6e1e1;
}

.home-icon-box image {
  width: 30rpx;
  height: 30rpx;
  margin: 0;
}


.title-custom-box {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}


.clinicIndex {
  width: 100vw;
  background: #ffffff;
}

.clinicIndex .header {
  position: relative;
  height: 382rpx;
}

.clinicIndex .header .bg {
  height: 130rpx;
  width: 100%;
  display: block;
  position: absolute;
}

.header .myInfo {
  width: 686rpx;
  height: 360rpx;
  position: absolute;
  top: 22rpx;
  left: 32rpx;
  z-index: 1;
}

.header .myInfo .infoBg {
  display: block;
  height: 100%;
  width: 100%;
  position: absolute;
}

.header .myInfo .infoContent {
  position: absolute;
  z-index: 2;
  width: 100%;
}

.infoContent .infoTop {
  padding-top: 50rpx;
  width: 100%;
  display: flex;
}

.infoContent .infoTop .headImg {
  height: 120rpx;
  width: 120rpx;
  margin: 0rpx 36rpx;
  border-radius: 60rpx;
}

.infoContent .infoTop .content {
  width: calc(100% - 353rpx);
  margin-right: 10rpx;
}

.infoContent .infoTop .content .top {
  display: flex;
  margin-top: 16rpx;
  align-items: center;
}

.infoContent .infoTop .content .name {
  font-size: 32rpx;
  font-weight: 600;
  color: rgba(0, 0, 0, 0.85);
  line-height: 32rpx;
  margin-right: 16rpx;
  padding-top: 3rpx;
}

.infoContent .infoTop .content .position {
  font-weight: 500;
  color: rgba(0, 0, 0, 0.65);
  line-height: 32rpx;
  font-size: 24rpx;
  margin-right: 16rpx;
}

.infoContent .infoTop .content .code {
  height: 32rpx;
  width: 32rpx;
}

.infoContent .infoTop .content .text {
  margin-top: 24rpx;
  font-size: 28rpx;
  font-weight: 600;
  text-align: left;
  color: rgba(0, 0, 0, 0.65);
  line-height: 32rpx;
}

.infoContent .infoTop .setup {
  width: 145rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 0.5);
  border: 1rpx solid #c8cdd8;
  border-radius: 200rpx 0rpx 0rpx 200rpx;
  float: right;
  display: flex;  
  align-items: center;
  margin-right: 8rpx;
}

.infoContent .infoTop .setup image {
  height: 32rpx;
  width: 32rpx;
  float: left;
  margin: 0 8rpx 0 26rpx;
}

.infoContent .infoTop .setup text {
  font-size: 28rpx;
  font-weight: 500;
  text-align: center;
  color: #000000;
}

.infoContent .statistics {
  margin-top: 48rpx;
  display: flex;
}

.infoContent .statistics .item {
  width: 33.3%;
  text-align: center;
}

.infoContent .statistics .item .num {
  font-size: 32rpx;
  font-weight: 600;
  color: rgba(0, 0, 0, 0.85);
  line-height: 32rpx;
}

.infoContent .statistics .item .name {
  margin-top: 24rpx;
  font-size: 24rpx;
  font-weight: 500;
  text-align: center;
  color: rgba(0, 0, 0, 0.65);
  line-height: 32rpx;
}

.clinicIndex .tabItem {
  height: 168rpx;
  padding: 0rpx 32rpx;
}

.clinicIndex .tabItem .scrollView {
  width: 100%;
  height: 168rpx;
  white-space: nowrap;
}

.clinicIndex .tabItem .item {
  width: calc(50% - 10rpx);
  position: relative;
  height: 168rpx;
  display: inline-block;
}

.clinicIndex .tabItem .margin_left_10 {
  margin-left: 10rpx;
}

.clinicIndex .tabItem .item .bg {
  height: 168rpx;
  width: calc(100% + 28rpx);
  position: absolute;
  left: -10rpx;
  top: -5rpx;
}

.clinicIndex .tabItem .item .content {
  position: relative;
  z-index: 1;
  height: 168rpx;
}

.clinicIndex .tabItem .item .content image {
  height: 72rpx;
  width: 72rpx;
  float: left;
  padding-top: 36rpx;
  padding-left: 54rpx;
}

.clinicIndex .tabItem .item .content text {
  font-size: 28rpx;
  font-weight: 600;
  text-align: center;
  color: rgba(0, 0, 0, 0.85);
  line-height: 28rpx;
  float: left;
  padding-top: 64rpx;
  margin-left: 32rpx;
}

.clinicIndex .groupList {
  padding-left: 32rpx;
  padding-bottom: 10rpx;
}

.clinicIndex .groupList .scrollView {
  max-height: calc(100vh - 699rpx);
}

.clinicIndex .groupList .item {
  margin-bottom: 32rpx;
  display: flex;
  position: relative;
}

.clinicIndex .groupList .item .headImg {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
}

.clinicIndex .groupList .item .right {
  width: calc(100% - 136rpx);
  margin-left: 24rpx;
  padding-bottom: 18rpx;
  padding-right: 32rpx;
  border-bottom: 1rpx solid #eeeeee;
}

.clinicIndex .groupList .item .right .top {
  overflow: hidden;
  padding-top: 4rpx;
}

.clinicIndex .groupList .item .right .groupName {
  font-weight: 600;
  text-align: left;
  color: rgba(0, 0, 0, 0.85);
  line-height: 32rpx;
  font-size: 28rpx;
  float: left;
  margin-right: 12rpx;
  max-width: 360rpx;
}

.clinicIndex .groupList .item .right .sex {
  height: 32rpx;
  width: 32rpx;
  float: left;
}

.clinicIndex .groupList .item .right .time {
  float: right;
  font-weight: 500;
  text-align: left;
  color: rgba(0, 0, 0, 0.85);
  line-height: 32rpx;
  font-size: 24rpx;
}

.clinicIndex .groupList .item .right .message {
  margin-top: 14rpx;
  font-weight: 500;
  text-align: left;
  color: rgba(0, 0, 0, 0.85);
  line-height: 32rpx;
  font-size: 24rpx;
  height:32rpx;
}

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.head-icon {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  bottom: 20rpx;
  left: 56rpx;
}
.nodata {
  display: flex;
  justify-content: center;
  align-items: center;
  height: calc(100vh - 699rpx);
  margin-right: 32rpx;
}
.nodata .nodata-img {
  height: 364rpx;
  width: 480rpx;
  display: block;
}
.nodata .nodata-text {
  text-align: center;
  display: block;
  margin-top: 114rpx;
  color: rgba(0,0,0,0.85);
  line-height: 32rpx;
  font-size: 32rpx;
}